{
  "name": "sparkify",
  "version": "0.2.1",
  "description": "Mintlify-style static docs generator for GitHub Pages",
  "license": "MIT",
  "homepage": "https://github.com/spark-aiur/sparkify",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spark-aiur/sparkify.git"
  },
  "bugs": {
    "url": "https://github.com/spark-aiur/sparkify/issues"
  },
  "keywords": [
    "docs",
    "mintlify",
    "astro",
    "openapi",
    "fastapi",
    "github-pages"
  ],
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "sparkify": "dist/bin.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "rm -rf dist && tsc -b tsconfig.json --force"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "sparkify-core": "0.2.1"
  }
}
